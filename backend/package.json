{
  "name": "aura-presence-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend-Server f√ºr Aura Presence mit Auth, Analyse und Stripe-Integration",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "db:migrate": "node src/database/migrate.js",
    "migrate": "knex migrate:latest --knexfile src/database/knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile src/database/knexfile.js",
    "migrate:make": "knex migrate:make --knexfile src/database/knexfile.js",
    "migrate:status": "knex migrate:status --knexfile src/database/knexfile.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "openai": "^4.24.1",
    "pg": "^8.11.3",
    "socket.io": "^4.8.3",
    "stripe": "^14.10.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  }
}
